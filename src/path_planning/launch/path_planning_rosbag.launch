<launch>
  <node
    pkg="path_planning"
    type="path_planning"
    name="pathPlanning" 
    output="screen"
  />
  <node
    pkg="rviz"
    type="rviz"
    name="rviz" 
    args="-d $(find path_planning)/rviz/config.rviz"
  />
  <node
    pkg="rosbag"
    type="play"
    name="player" 
    output="screen"
    args="$(find path_planning)/bags/$(arg bag_name)"
  />

</launch> 
